{
	"name": "RPI SIS Redesign",
	"version": "1.0",
	"description": "An extension that changes the superficial aspects of Rensselaer's Student Information System website.",

	"content_scripts": [
		{
			"matches": [
				"http://sis.rpi.edu/*",
				"https://sis.rpi.edu/*"
			],
			"js": [
				"js/functionDump.js",
				"js/landingPage.js",
				"js/content.js",
				"js/personalInformationMenu.js",
				"js/financialAidMenu.js",
				"js/mainMenu.js",
				"js/hrPayrollMenu.js",
				"js/studentMenu.js",
				"js/taxForms.js",
				"js/payInformation.js",
				"js/benefitsAndDeductions.js",
				"js/studentAccountDetailMenu.js",
				"js/registerAddDrop.js",
				"js/checkRegistrationStatus.js",
				"js/viewAdvisorCurriculum.js",
				"js/changeThesis.js",
				"js/viewGrades.js",
				"js/viewTranscript.js",
				"js/summaryBySemester.js",
				"js/timeSheet.js",
				"js/view_w4.js",
				"js/stateTaxExemptions.js",
				"js/wageTheft.js",
				"js/benefitsAndDeductionsPages.js",
				"js/footer.js"
			],
			"css": [
				"css/content.css"
			],
			"run_at": "document_idle"
		}
	],

	"manifest_version": 2
}
