{
	"name": "RPI SIS Redesign",
	"version": "1.0",
	"description": "An extension that changes the superficial aspects of Rensselaer's Student Information System website.",

	"content_scripts": [
		{
			"matches": [
				"http://sis.rpi.edu/*",
				"https://sis.rpi.edu/*"
			],
			"js": [
				"functionDump.js",
				"landingPage.js",
				"content.js",
				"personalInformationMenu.js",
				"financialAidMenu.js",
				"mainMenu.js",
				"hrPayrollMenu.js",
				"studentMenu.js",
				"taxForms.js",
				"payInformation.js",
				"benefitsAndDeductions.js",
				"studentAccountDetailMenu.js",
				"registerAddDrop.js",
				"checkRegistrationStatus.js",
				"viewAdvisorCurriculum.js",
				"changeThesis.js",
				"viewGrades.js",
				"viewTranscript.js",
				"summaryBySemester.js",
				"timeSheet.js",
				"view_w4.js",
				"stateTaxExemptions.js",
				"footer.js"
			],
			"css": [
				"content.css"
			],
			"run_at": "document_end"
		}
	],

	"manifest_version": 2
}
